<Page x:Class="Warehouser_NET.Page_Popped"
      xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
      xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
      xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
      xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
      xmlns:local="clr-namespace:Warehouser_NET"
      mc:Ignorable="d" 
      d:DesignHeight="450" d:DesignWidth="800"
      Title="Page_Popped" FontFamily="Microsoft YaHei" Foreground="{DynamicResource FontColorBrush}" Loaded="Page_Loaded">

    <Grid  x:Name="BaseGrid">
        <Label Content="此页已弹出，请在弹出的窗口操作" FontFamily="Microsoft YaHei" FontSize="36" FontStyle="Italic" HorizontalAlignment="Center" VerticalAlignment="Center"/>
    </Grid>
</Page>
