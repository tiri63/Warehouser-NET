<Page x:Class="Warehouser_NET.Page_About"
      xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
      xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
      xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
      xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
      xmlns:local="clr-namespace:Warehouser_NET"
      mc:Ignorable="d" 
      d:DesignHeight="450" d:DesignWidth="800"
      Title="Page_About" FontFamily="Microsoft YaHei" Foreground="{DynamicResource FontColorBrush}" Loaded="Page_Loaded" >

    <Grid x:Name="BaseGrid" VerticalAlignment="Stretch" HorizontalAlignment="Stretch">
        <Label x:Name="Title_Label" Content="关于" VerticalAlignment="Top" HorizontalAlignment="Left" FontStyle="Italic" FontSize="20"/>
        <Button x:Name="ResetBtn" Content="重置所有数据" HorizontalAlignment="Left" Margin="10,56,0,0" VerticalAlignment="Top" Height="22" Width="100" Click="Button_Click"/>
    </Grid>
</Page>
